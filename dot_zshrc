source <(antibody init)
ANTIBODY_HOME="$(antibody home)"

autoload -Uz compinit 

ZSH="$ANTIBODY_HOME"/https-COLON--SLASH--SLASH-github.com-SLASH-robbyrussell-SLASH-oh-my-zsh
ZSH_CACHE_DIR="$ANTIBODY_HOME/zsh_cache"
mkdir -p $ZSH_CACHE_DIR

_omz_plugin() {
    local plugin=$1
    antibody bundle robbyrussell/oh-my-zsh path:plugins/$plugin
}

autoload -U bashcompinit
bashcompinit
for d in $HOME/.zshrc.d/*; do
    source $d
done

antibody bundle zsh-users/zsh-syntax-highlighting
antibody bundle zsh-users/zsh-completions
antibody bundle robbyrussell/oh-my-zsh path:lib
antibody bundle robbyrussell/oh-my-zsh path:themes/robbyrussell.zsh-theme

compinit -i

# >>> conda initialize >>>
# !! Contents within this block are managed by 'conda init' !!
__conda_setup="$('/home/xing/.conda/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/xing/.conda/etc/profile.d/conda.sh" ]; then
        . "/home/xing/.conda/etc/profile.d/conda.sh"
    else
        export PATH="/home/xing/.conda/bin:$PATH"
    fi
fi
unset __conda_setup
# <<< conda initialize <<<

